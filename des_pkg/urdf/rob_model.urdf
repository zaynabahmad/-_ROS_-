<?xml version="0.0"?>

<robot name="rob_rib" xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find des_pkg)/urdf/materials.xacro"/>
<xacro:include filename="$(find des_pkg)/urdf/plugins.gazebo"/>
<xacro:property name="camera_link" value="0.05" /> <!-- Size of square 'camera' box -->

 <link name="base_link">
  <visual>
   <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
   <geometry>
    <box size="0.2 0.1 0.05"/>
   </geometry>
   <material name="grey"/>
  </visual>
  <inertial>
   <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
   <mass value="0.0"/>
   <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
   

  </inertial>
  <collision>
     <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
     <geometry>
        <box size="0.2 0.1 0.05"/>
     </geometry>
     
  </collision>
 </link>
   <!--upper_left_wheel-->
   <link name="upper_left_wheel">
    
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
    </collision>
    <visual name="upper_left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <inertia
        ixx=".1" ixy="0.0" ixz="0.0"
        iyy=".1" iyz="0.0"
        izz=".1"/>
    </inertial>
  </link>
  <!--upper_right_wheel-->
   <link name="upper_right_wheel">
    
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
    </collision>
    <visual name="upper_right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <inertia
        ixx=".1" ixy="0.0" ixz="0.0"
        iyy=".1" iyz="0.0"
        izz=".1"/>
    </inertial>
  </link>
  <!--back_left_wheel-->
  <link name="back_left_wheel">
    
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
    </collision>
    <visual name="back_left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <inertia
        ixx=".1" ixy="0.0" ixz="0.0"
        iyy=".1" iyz="0.0"
        izz=".1"/>
    </inertial>
  </link>
  <!--back_right_wheel-->
  <link name="back_right_wheel">
    
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
    </collision>
    <visual name="back_right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <inertia
        ixx=".1" ixy="0.0" ixz="0.0"
        iyy=".1" iyz="0.0"
        izz=".1"/>
    </inertial>
  </link>
  <!--joints-->
  <!--back_left_wheel_joint-->

  <joint type="continuous" name="back_left_wheel_joint">
    <origin xyz="0.05 0.05 -0.025" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="back_left_wheel"/>
    <axis xyz="1 1 0" rpy="0 0 0"/>
    <!-- <limit effort="10000" velocity="1000"/> -->
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
  <!--back_right_wheel_joint-->

  <joint type="continuous" name="back_right_wheel_joint">
    <origin xyz="-0.05 0.05 -0.025" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="back_right_wheel"/>
    <axis xyz="1 1 0" rpy="0 0 0"/>
    <!-- <limit effort="10000" velocity="1000"/> -->
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
  <!--upper_right_wheel_joint-->
  <joint type="continuous" name="upper_right_wheel_joint">
    <origin xyz="-0.05 -0.05 -0.025" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="upper_right_wheel"/>
    <axis xyz="1 1 0" rpy="0 0 0"/>
    <!-- <limit effort="10000" velocity="1000"/> -->
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
  <!--upper_left_wheel_joint-->
  <joint type="continuous" name="upper_left_wheel_joint">
    <origin xyz="0.05 -0.05 -0.025" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="upper_left_wheel"/>
    <axis xyz="1 1 0" rpy="0 0 0"/>
    <!-- <limit effort="10000" velocity="1000"/> -->
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>


  <!-- Camera -->
  <link name="camera_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
    <box size="${camera_link} ${camera_link} ${camera_link}"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
    <box size="${camera_link} ${camera_link} ${camera_link}"/>
      </geometry>
      <material name="red"/>
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

   <joint name="camera_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="${camera_link} 0 ${height3 - axel_offset*2}" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="camera_link"/>
  </joint>
    





</robot>